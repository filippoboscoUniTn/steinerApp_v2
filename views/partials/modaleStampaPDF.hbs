<div class="modal fade" id="modaleStampaPDF" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">

    <div class="modal-content">

      <div class="modal-header bg-primary">

        <h4 class="modal-title text-white" id="modalTitleStampaPDF"></h4>
        <button type="button" class="btn btn-danger" onclick="closeModaleStampa()" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>

      </div>

      <div class="modal-body">

        <form method="POST" id="formStampaPDF">


                    <label for="semestre">Semestre</label>
                    <select class="form-control" name="semestre" id='semestre'>
                        <option value=1>1 Semestre</option>
                        <option value=2>2 Semestre</option>
                    </select>
                    <label for="fontSize">Font Size</label>
                    <select class="form-control" name="fontSize" id='font'>
                        <option>6</option>
                        <option>7</option>
                        <option>8</option>
                        <option>9</option>
                        <option selected="true">10</option>
                        <option>11</option>
                        <option>12</option>
                        <option>13</option>
                        <option>14</option>
                        <option>15</option>
                        <option>16</option>
                        <option>17</option>
                        <option>18</option>
                    </select>
                    <div class="mt-3">
                      <button type="submit" id="btnStampaPDF" class="btn btn-success">Stampa PDF</button>
                    </div>
        </form>

      </div>

    </div>

  </div>
</div>
